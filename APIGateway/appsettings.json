{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "spring": {
    "application": {
      "name": "gw-service"
    }

  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": false,
      "validateCertificates": false,
      "registryFetchIntervalSeconds": 10,
      "instanceInfoReplicationIntervalSeconds": 10
    },
    "instance": {
      "port": 9000,
      "instanceId": "localhost:9000",
      "hostName": "localhost"
    }
  }
}